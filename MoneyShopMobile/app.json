{
  "expo": {
    "name": "MoneyShop",
    "slug": "moneyshop-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.moneyshop.mobile",
      "infoPlist": {
        "NSCameraUsageDescription": "Această aplicație are nevoie de acces la cameră pentru a încărca poze cu buletinul pentru verificare KYC.",
        "NSPhotoLibraryUsageDescription": "Această aplicație are nevoie de acces la galeria foto pentru a încărca poze cu buletinul pentru verificare KYC."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.moneyshop.mobile",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "build": {
        "babel": {
          "include": ["@tanstack/react-query"]
        }
      }
    }
  }
}

